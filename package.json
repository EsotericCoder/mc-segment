{
  "name": "rollup-umd",
  "version": "1.0.0",
  "description": "A simple starter for UMD libraries",
  "main": "dist/bundle.js",
  "license": "MIT",
  "config": {
    "moduleName": "Library"
  },
  "scripts": {
    "start": "rollup -c --watch",
    "build": "rollup -c --environment PROD:true"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "uglify-js": "^2.8.18"
  }
}
